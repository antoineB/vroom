material Ford/Top
{
  technique
  {
    pass
    {
      // Disables the culling of the back of the face:
      cull_hardware none
      cull_software none

          ambient   0.8 0.5 0.0 1.0
      	  emissive  0.5 0.5 0.0 1.0
   
        texture_unit
        {
        // Loading external image texture:
        texture ford_top.jpg
        }
    }
  }
}

