material Ford/TopWindow
{
  technique
  {
    pass
    {
      // Disables the culling of the back of the face:
      cull_hardware none
      cull_software none

       lighting on
       scene_blend alpha_blend
       depth_write off

       ambient   0.0 0.0 0.2 1.0
       emissive  0.1 0.0 0.3 1.0
   
        texture_unit
        {
        // Loading external image texture:
        texture ford_top_window.jpg
	alpha_op_ex source1 src_manual src_texture 0.4
        }
    }
  }
}

